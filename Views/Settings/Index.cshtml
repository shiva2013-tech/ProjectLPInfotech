@model IEnumerable<ProjectLPInfotech.Models.Setting>

@{
    ViewData["Title"] = "Index";
}

<div id="PlaceHolderHere"></div>
<p>
    <button type="button" class="btn btn-primary" data-toggle="ajax-modal" data-target="#addData" data-url="@Url.Action("Create")">
        Create New
    </button>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Key)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Value)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Value2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Description)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Created)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastModified)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IsDeleted)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Key)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Value)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Value2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Description)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Created)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LastModified)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IsDeleted)
                </td>
                <td>
                    <button type="button" class="btn btn-primary" data-toggle="ajax-modal" data-target="#editData" data-url="@Url.Action($"Edit/{item.Id}")">
                        Edit
                    </button>
                    <button type="button" class="btn btn-primary" data-toggle="ajax-modal" data-target="#detailData" data-url="@Url.Action($"Details/{item.Id}")">
                        Details
                    </button>
                    <button type="button" class="btn btn-primary" data-toggle="ajax-modal" data-target="#addData" data-url="@Url.Action($"Delete/{item.Id}")">
                        Delete
                    </button>
                </td>
            </tr>
        }
    </tbody>
</table>

